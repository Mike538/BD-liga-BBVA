select * from partido;

ALTER TABLE Equipo ADD CONSTRAINT pk_equipo PRIMARY KEY (ID_Equipo);

-- Para la tabla "Jugador"
ALTER TABLE Jugadores ADD CONSTRAINT pk_jugador PRIMARY KEY (ID_Jugador);

--dt
ALTER TABLE DirectorTecnico ADD CONSTRAINT pk_director_tecnico PRIMARY KEY 
(ID_Dt);

--a.t
ALTER TABLE AuxiliarTecnico ADD CONSTRAINT pk_auxiliar_tecnico PRIMARY KEY 
(ID_Auxiliar_Tec);

--EStadio
ALTER TABLE Estadio ADD CONSTRAINT pk_estadio PRIMARY KEY (ID_Estadio);

--Partido
ALTER TABLE Partido ADD CONSTRAINT pk_partido PRIMARY KEY (ID_Partido);

-- Agregar las restricciones de clave externa para la tabla "Partido"
ALTER TABLE Partido ADD CONSTRAINT fk_equipo_local FOREIGN KEY (Equipo_Local) REFERENCES Equipo(ID_Equipo);
ALTER TABLE Partido ADD CONSTRAINT fk_equipo_visitante FOREIGN KEY (Equipo_Visitante) REFERENCES Equipo(ID_Equipo);

-- Para la tabla "Estadisticas"
ALTER TABLE Estadisticas ADD CONSTRAINT pk_estadisticas PRIMARY KEY (ID_Estadistica);

-- Para la tabla "Arbitro"
ALTER TABLE Arbitro ADD CONSTRAINT pk_arbitro PRIMARY KEY (ID_Arbitro);

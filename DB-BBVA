-- Crear la tabla "DirectorTecnico"
CREATE TABLE DirectorTecnico (
    ID_Dt NUMBER,
    Nombre VARCHAR2(50),
    Apellido_Pat VARCHAR2(50),
    Apellido_Mat VARCHAR2(50),
    Apodo VARCHAR2(50),
    Fecha_Nacimiento DATE
    Edad NUMBER,
    Tiempo_Contrato NUMBER
);


-- Crear la tabla "Equipo"
CREATE TABLE Equipo (
    ID_Equipo NUMBER,
    Nombre VARCHAR2(100),
    Apodo VARCHAR2(50),
    Antiguedad NUMBER,
    Ciudad VARCHAR2(100)
);


CREATE TABLE Jugadores (
    ID_Jugador NUMBER,
    Nombre VARCHAR2(50),
    Apellido_Pat VARCHAR2(50),
    Apellido_Mat VARCHAR2(50),
    Edad NUMBER,
    Lugar_Nacimiento VARCHAR2(100),
    Apodo VARCHAR2(50),
    Dorsal NUMBER,
    Posicion VARCHAR2(50),
    Precio_Mercado NUMBER
);


-- Crear la tabla "Equipo"
CREATE TABLE Equipo (
    ID_Equipo VARCHAR(25),
    Nombre VARCHAR2(100),
    Apodo VARCHAR2(50),
    Antiguedad NUMBER,
    Ciudad VARCHAR2(100)
);
